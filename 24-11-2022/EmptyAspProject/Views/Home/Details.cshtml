
@model System.Collections.Generic.IEnumerable<EmptyAspProject.Modal.Employee>;


@{
    Layout = "~/Views/Shared/_Layout.cshtml";
    ViewBag.Title = "Employee details page";
}

<div>

    @foreach(var employee in Model) {
       <div class="card m-3 row">
        <img class="card-img-top" width="100%" src="~/images/profilePicture.png" height="150px"></img>
        <div class="card-footer text-center col-4">
        <h1 class="text-danger">@employee.name</h1>
     

        <h1>@employee.email</h1>
        <h1>@employee.id</h1>
        </div>
       </div>

    }
</div>

@section scripts {
    <script src="~/js/CustomJavascript.js"></script>
}








